{
   "name": "personal_folio",
   "version": "0.1.0",
   "private": true,
   "dependencies": {
      "@testing-library/jest-dom": "^4.2.4",
      "@testing-library/react": "^9.3.2",
      "@testing-library/user-event": "^7.1.2",
      "@types/jest": "^24.0.0",
      "@types/node": "^14.14.10",
      "@types/react": "17.0.0",
      "@types/react-dom": "^17.0.0",
      "csscomb": "^4.3.0",
      "eslint-config-prettier": "6.15.0",
      "formik": "2.2.5",
      "husky": "^4.3.0",
      "lint-staged": "^10.5.2",
      "node": "^14.15.0",
      "node-sass": "^4.14.0",
      "prettier": "^2.2.0",
      "react": "^17.0.1",
      "react-dom": "^17.0.1",
      "react-scripts": "3.4.3",
      "sass": "^1.29.0",
      "typescript": "4.1.2"
   },
   "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "format": "prettier --w \"src/**/*.{ts,tsx}\" && eslint --fix . --ext .ts,.tsx"
   },
   "eslintConfig": {
      "extends": [
         "react-app",
         "prettier"
      ],
      "rules": {
         "no-multi-spaces": "error",
         "jsx-quotes": [
            "warn",
            "prefer-single"
         ],
         "no-mixed-spaces-and-tabs": "error",
         "no-debugger": "warn",
         "no-alert": "warn",
         "no-console": "warn",
         "prefer-const": "error"
      }
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   },
   "lint-staged": {
      "*.{js,jsx,ts,tsx}": [
         "prettier --write",
         "eslint --fix"
      ]
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   }
}
